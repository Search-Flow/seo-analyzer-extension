{
  "manifest_version": 3,
  "name": "SEO Analyzer",
  "version": "1.0",
  "description": "A simple SEO analysis tool for Chrome.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["script.js", "style.css", "incomingLinks.js", "logo.png"],
      "matches": ["<all_urls>"]
    }
  ]
}